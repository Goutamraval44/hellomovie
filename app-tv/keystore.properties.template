# Keystore configuration for signing Android TV app
# Copy this file to keystore.properties and update with your actual values

# Path to your keystore file (relative to app-tv directory)
storeFile=vega-tv-release.keystore

# Password for the keystore
storePassword=your_keystore_password

# Key alias name
keyAlias=vega-tv-key

# Password for the key
keyPassword=your_key_password

# Optional: Store type (default is JKS)
# storeType=JKS 